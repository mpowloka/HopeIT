<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.hopeit.charapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.hopeit.charapp.MainActivity">
        </activity>
        <activity android:name="org.hopeit.charapp.MakerOrHelpActivity" />
        <activity android:name="org.hopeit.charapp.MakerActivity" />
        <activity android:name="org.hopeit.charapp.HelpActivity" />
        <activity
            android:name="org.hopeit.charapp.SplashActivity"
            android:theme="@style/SplashTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>